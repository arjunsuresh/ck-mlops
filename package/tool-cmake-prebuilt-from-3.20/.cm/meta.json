{
  "check_exit_status": "yes",
  "customize": {
    "extra_dir": "",
    "install_env": {
      "*PACKAGE_URL": "https://github.com/Kitware/CMake/releases/download/v$<<CMAKE_VERSION>>$",
      "PACKAGE_WGET": "YES",
      "PACKAGE_WGET_EXTRA": "--no-cookies",
      "PACKAGE_COPY": "NO",
      "PACKAGE_DETECT_VARS": "YES",
      "PACKAGE_GIT": "NO",
      "PACKAGE_SKIP_LINUX_MAKE": "YES"
    }
  },
  "end_full_path": {
    "linux": "prebuilt/bin/cmake",
    "macos": "prebuilt/CMake.app/Contents/bin/cmake",
    "win": "prebuilt\\bin\\cmake.exe"
  },
  "need_cpu_info": "yes",
  "only_for_host_os_tags": [
    "linux",
    "windows"
  ],
  "only_for_target_os_tags": [
    "linux",
    "windows"
  ],
  "process_script": "install",
  "soft_uoa": "77ef5e31404d9589",
  "suggested_path": "tool-cmake",
  "tags": [
    "tool",
    "cmake",
    "downloaded",
    "prebuilt",
    "from-github",
    "v3"
  ],
  "use_scripts_from_another_entry": {
    "data_uoa": "cd9ccc74060b3d18",
    "module_uoa": "script"
  },
  "variations": {
    "v3.21.1": {
      "extra_env": {
        "CMAKE_VERSION": "3.21.1"
      },
      "extra_tags": [
        "v3.21.1"
      ],
      "on_by_default": "yes"
    }
  }
}
